<highlightDefinition version="1.2">
	<name>Yuma</name>
	<blockStartMarker indent="1">^\s*&lt;\?yuma|^\s*(&lt;[^!?%/](?!.+?(/&gt;|&lt;/.+?&gt;))|&lt;[!%]--(?!.+?--%?&gt;)|&lt;%[!]?(?!.+?%&gt;))|^\s*(?:if.+?then\s*(?!.+?)|for|while|class|module|select|^\s*(?:protected|private)?\s*(?&lt;!end)\s*(?:sub|function))</blockStartMarker>
	
	<symbols>
		<symbol type="Class">
			<entryRegEx>^\s*class\s+\w+\s*</entryRegEx>
		</symbol>
		<symbol type="Method">
			<entryRegEx>^\s*(?:protected|private)?\s*(?&lt;!end)\s*(?:sub|function).+?\([^\)]*\)\s*(?:as\s*\w+\s*)?</entryRegEx>
		</symbol>
		<symbol type="Module">
			<entryRegEx>^\s*module\s+\w+\s*</entryRegEx>
		</symbol>		
	</symbols>
	
	<blockEndMarker>^\s*\?&gt;|^\s*(&lt;/[^&gt;]+&gt;|[/%]&gt;|--&gt;)\s*$|^\s*(?:end|wend|next)</blockEndMarker>
